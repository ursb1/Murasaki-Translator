{
  "name": "murasaki-translator",
  "version": "2.0.0",
  "main": "./out/main/index.js",
  "author": "soundstarrain",
  "description": "Murasaki Local AI Translator",
  "repository": {
    "type": "git",
    "url": "https://github.com/soundstarrain/Murasaki-Translator.git"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "electron-vite dev",
    "dev:debug": "electron-vite dev --inspect --sourcemap",
    "dev:debug:brk": "electron-vite dev --inspect-brk --sourcemap",
    "build": "electron-vite build",
    "build:win": "npm run build && electron-builder --win --x64",
    "preview": "electron-vite preview",
    "start": "electron-vite preview"
  },
  "build": {
    "appId": "com.murasaki.translator",
    "productName": "Murasaki Translator",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "zip",
      "icon": "resources/icon.png",
      "extraResources": [
        {
          "from": "../python_env",
          "to": "python_env",
          "filter": [
            "**/*",
            "!**/__pycache__/*",
            "!**/Scripts/*.pdb",
            "!**/Lib/test/*",
            "!get-pip.py"
          ]
        }
      ]
    },
    "mac": {
      "target": "dmg",
      "icon": "resources/icon.icns",
      "category": "public.app-category.productivity",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "linux": {
      "target": "AppImage",
      "icon": "resources/icon.png",
      "category": "Office"
    },
    "generateUpdatesFilesForAllChannels": false,
    "publish": null,
    "electronLanguages": [
      "en",
      "zh-CN",
      "zh_CN",
      "ja"
    ],
    "extraFiles": [
      {
        "from": "../release/LICENSE.Murasaki Translator.txt",
        "to": "LICENSE.txt"
      },
      {
        "from": "../release/readme.md",
        "to": "README.md"
      }
    ],
    "extraResources": [
      {
        "from": "../middleware",
        "to": "middleware",
        "filter": [
          "**/*",
          "!**/__pycache__/*",
          "!**/*.spec",
          "!**/*.log",
          "!**/hf_cache/*",
          "!**/models/*.gguf",
          "!**/LLaMA-Factory/*",
          "!**/.git*",
          "!**/.gitignore",
          "!**/*.log",
          "!**/temp_*.json",
          "!**/.venv",
          "!**/.venv/**",
          "!**/temp_progress.jsonl",
          "!**/*.diff",
          "!**/*.bat",
          "!**/llama-b*-bin-*/**",
          "!**/server.log",
          "!**/test_*.py",
          "!**/tests/**",
          "!**/__tests__/**",
          "!**/*.test.*"
        ]
      }
    ],
    "afterPack": "./scripts/afterPack.js"
  },
  "dependencies": {
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/view": "^6.39.14",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@uiw/react-codemirror": "^4.25.4",
    "@xyflow/react": "^12.10.0",
    "chokidar": "^3.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dagre": "^0.8.5",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/dagre": "^0.7.53",
    "@types/node": "^18.19.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.6.0"
  }
}